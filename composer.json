{
    "name": "harikt/dms-expressive-skeleton",
    "description": "Zend expressive skeleton with dms. Begin developing PSR-7 middleware applications in seconds!",
    "type": "project",
    "homepage": "https://github.com/harikt/dms-expressive-skeleton",
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "aura/session": "^2.1",
        "cache/cache": "^0.1.0",
        "dms-org/package.analytics": "^0.8",
        "dms-org/package.blog": "^0.8",
        "dms-org/package.contact-us": "^0.8.0",
        "dms-org/package.content": "^0.8.0",
        "dms-org/package.faqs": "^0.8.0",
        "harikt/web.expressive": "dev-master",
        "illuminate/cache": "^5.4",
        "illuminate/config": "^5.4",
        "illuminate/container": "^5.4",
        "illuminate/events": "^5.4",
        "illuminate/filesystem": "^5.4",
        "illuminate/view": "^5.4",
        "josegonzalez/dotenv": "^3.1",
        "league/oauth2-google": "^1.0",
        "marcj/topsort": "^1.0",
        "roave/security-advisories": "dev-master",
        "symfony/console": "^3.3",
        "zendframework/zend-component-installer": "^1.0",
        "zendframework/zend-config-aggregator": "^1.0",
        "zendframework/zend-expressive": "^2.0.2",
        "zendframework/zend-expressive-fastroute": "^2.0",
        "zendframework/zend-expressive-helpers": "^4.0",
        "zendframework/zend-servicemanager": "^3.3",
        "zendframework/zend-stdlib": "^3.1"
    },
    "require-dev": {
        "filp/whoops": "^2.1.7",
        "heroku/heroku-buildpack-php": "^124.0",
        "phpunit/phpunit": "^6.0.8 || ^5.7.15",
        "robmorgan/phinx": "^0.9.1",
        "squizlabs/php_codesniffer": "^2.8.1",
        "zendframework/zend-expressive-tooling": "^0.4.1",
        "zfcampus/zf-development-mode": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/src/"
        },
        "files" : [
            "./src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/AppTest/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "check": [
            "@cs-check",
            "@test"
        ],
        "clear-config-cache": "php bin/clear-config-cache.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public public/index.php",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v"
    }
}
